{
	"name": "<%= moduleName %>",
	"version": "0.0.0",
	"description": "<%= moduleDescription %>",
	"license": "MIT",
	"repository": "<%= githubUsername %>/<%= repoName %>",
	"author": {
		"name": "<%= name %>",
		"email": "<%= email %>",
		"url": "<%= humanizedWebsite %>"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
    "setup": "fly setup",
    "build": "<% if (esnext) { %>BABEL=1 <% } %>fly build",
    "release": "<% if (esnext) { %>BABEL=1 <% } %>fly release",
    "start": "<% if (esnext) { %>BABEL=1 <% } %>fly release serve",
    "watch": "<% if (esnext) { %>BABEL=1 <% } %>fly watch"
    "test": "xo<% if (ava) { %>&& ava<% } %>"
  },
	"main": "lib/index.js",
	"files": [
		"lib"
	],
	"keywords": [
		"d3",
		"d3.js",
		"chart"
	],
	"dependencies": {
		"d3": "^4.3.0",
		"object-assign": "^4.1.0"
	},
  "devDependencies": {<% if (ava) { %>
    "ava": "^0.15.2",<% } %>
    "browser-module-env": "^0.0.4",
    "xo": "^0.16.0"
  },
  "fly": {
    "pkg": "node_modules/browser-module-env"
  },
  "xo": {
    "envs": ["browser"]
  }
}
